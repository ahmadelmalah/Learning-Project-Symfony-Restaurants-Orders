{% if paginator.hasMoreThanOnePage() %}
<nav aria-label=\"Page navigation\">
  <ul class=\"pagination\">
    {% if paginator.hasPreviousPage() %}
      <li><a href=\"#\" onclick=\"getOrders( {{ 1 }} )\">&lt;&lt;</a></li>
      <li><a href=\"#\" onclick=\"getOrders( {{ paginator.getPreviousPageNumber() }} )\">&lt;</a></li>
    {% endif %}

    {% for i in 1..paginator.getLastPageNumber() %}
      {% if i == paginator.getCurrentPageNumber() %}
        <li class=\"active\"><a href=\"#\" onclick=\"getOrders( {{ i }} )\">{{ i }}</a></li>
      {% else %}
        <li><a href=\"#\" onclick=\"getOrders( {{ i }} )\">{{ i }}</a></li>
      {% endif %}
    {% endfor %}

    {% if paginator.hasNextPage() %}
      <li><a href=\"#\" onclick=\"getOrders( {{ paginator.getNextPageNumber() }} )\">&gt;</a></li>
      <li><a href=\"#\" onclick=\"getOrders( {{ paginator.getLastPageNumber() }} )\">&gt;&gt;</a></li>
    {% endif %}
  </ul>
</nav>
{% endif %}
